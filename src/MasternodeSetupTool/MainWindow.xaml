<Window x:Class="MasternodeSetupTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Stratis Masternode Setup Tool" Name="MainWindowControl" Height="450" Width="800">
    <DockPanel>
        <StackPanel Name="Main" DockPanel.Dock="Top" Orientation="Vertical" Height="380" Width="800">
            <StackPanel.Background>
                <ImageBrush ImageSource="/Logo.png"/>
            </StackPanel.Background>
        </StackPanel>
        <StatusBar DockPanel.Dock="Bottom">
            <StatusBarItem>
                <TextBlock Name="txtStatusBar" Text="" />
            </StatusBarItem>
        </StatusBar>
    </DockPanel>
</Window>
